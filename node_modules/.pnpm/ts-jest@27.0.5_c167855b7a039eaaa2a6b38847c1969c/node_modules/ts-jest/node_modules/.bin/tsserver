#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/typescript@4.4.3/node_modules/typescript/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/typescript@4.4.3/node_modules/typescript/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/typescript@4.4.3/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules:/Users/zhangpian/Documents/projects/github/node_modules:/Users/zhangpian/Documents/projects/node_modules:/Users/zhangpian/Documents/node_modules:/Users/zhangpian/node_modules:/Users/node_modules:/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/ts-jest@27.0.5_c167855b7a039eaaa2a6b38847c1969c/node_modules/ts-jest/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/ts-jest@27.0.5_c167855b7a039eaaa2a6b38847c1969c/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/typescript@4.4.3/node_modules/typescript/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/typescript@4.4.3/node_modules/typescript/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/typescript@4.4.3/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules:/Users/zhangpian/Documents/projects/github/node_modules:/Users/zhangpian/Documents/projects/node_modules:/Users/zhangpian/Documents/node_modules:/Users/zhangpian/node_modules:/Users/node_modules:/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/ts-jest@27.0.5_c167855b7a039eaaa2a6b38847c1969c/node_modules/ts-jest/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/ts-jest@27.0.5_c167855b7a039eaaa2a6b38847c1969c/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../typescript@4.4.3/node_modules/typescript/bin/tsserver" "$@"
else
  exec node  "$basedir/../../../../../typescript@4.4.3/node_modules/typescript/bin/tsserver" "$@"
fi
