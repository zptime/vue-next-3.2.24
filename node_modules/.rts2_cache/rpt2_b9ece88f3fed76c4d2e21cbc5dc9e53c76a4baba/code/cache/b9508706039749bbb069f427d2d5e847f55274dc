{"code":"import { createObjectProperty, createSimpleExpression } from '@vue/compiler-core';\r\nimport { createDOMCompilerError } from '../errors';\r\nexport const transformVHtml = (dir, node, context) => {\r\n    const { exp, loc } = dir;\r\n    if (!exp) {\r\n        context.onError(createDOMCompilerError(50 /* X_V_HTML_NO_EXPRESSION */, loc));\r\n    }\r\n    if (node.children.length) {\r\n        context.onError(createDOMCompilerError(51 /* X_V_HTML_WITH_CHILDREN */, loc));\r\n        node.children.length = 0;\r\n    }\r\n    return {\r\n        props: [\r\n            createObjectProperty(createSimpleExpression(`innerHTML`, true, loc), exp || createSimpleExpression('', true))\r\n        ]\r\n    };\r\n};\r\n//# sourceMappingURL=vHtml.js.map","references":["/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/compiler-core/src/index.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/compiler-dom/src/errors.ts"],"map":"{\"version\":3,\"file\":\"vHtml.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../packages/compiler-dom/src/transforms/vHtml.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAEL,oBAAoB,EACpB,sBAAsB,EACvB,MAAM,oBAAoB,CAAA;AAC3B,OAAO,EAAE,sBAAsB,EAAiB,MAAM,WAAW,CAAA;AAEjE,MAAM,CAAC,MAAM,cAAc,GAAuB,CAAC,GAAG,EAAE,IAAI,EAAE,OAAO,EAAE,EAAE;IACvE,MAAM,EAAE,GAAG,EAAE,GAAG,EAAE,GAAG,GAAG,CAAA;IACxB,IAAI,CAAC,GAAG,EAAE;QACR,OAAO,CAAC,OAAO,CACb,sBAAsB,kCAAuC,GAAG,CAAC,CAClE,CAAA;KACF;IACD,IAAI,IAAI,CAAC,QAAQ,CAAC,MAAM,EAAE;QACxB,OAAO,CAAC,OAAO,CACb,sBAAsB,kCAAuC,GAAG,CAAC,CAClE,CAAA;QACD,IAAI,CAAC,QAAQ,CAAC,MAAM,GAAG,CAAC,CAAA;KACzB;IACD,OAAO;QACL,KAAK,EAAE;YACL,oBAAoB,CAClB,sBAAsB,CAAC,WAAW,EAAE,IAAI,EAAE,GAAG,CAAC,EAC9C,GAAG,IAAI,sBAAsB,CAAC,EAAE,EAAE,IAAI,CAAC,CACxC;SACF;KACF,CAAA;AACH,CAAC,CAAA\"}"}
