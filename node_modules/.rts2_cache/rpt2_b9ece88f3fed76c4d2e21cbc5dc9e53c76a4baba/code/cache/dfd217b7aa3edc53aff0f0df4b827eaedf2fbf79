{"code":"import { toHandlerKey, isObject } from '@vue/shared';\r\nimport { warn } from '../warning';\r\n/**\r\n * For prefixing keys in v-on=\"obj\" with \"on\"\r\n * @private\r\n */\r\nexport function toHandlers(obj) {\r\n    const ret = {};\r\n    if (__DEV__ && !isObject(obj)) {\r\n        warn(`v-on with no argument expects an object value.`);\r\n        return ret;\r\n    }\r\n    for (const key in obj) {\r\n        ret[toHandlerKey(key)] = obj[key];\r\n    }\r\n    return ret;\r\n}\r\n//# sourceMappingURL=toHandlers.js.map","references":["/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/shared/src/index.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-core/src/warning.ts"],"map":"{\"version\":3,\"file\":\"toHandlers.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../packages/runtime-core/src/helpers/toHandlers.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,YAAY,EAAE,QAAQ,EAAE,MAAM,aAAa,CAAA;AACpD,OAAO,EAAE,IAAI,EAAE,MAAM,YAAY,CAAA;AAEjC;;;GAGG;AACH,MAAM,UAAU,UAAU,CAAC,GAAwB;IACjD,MAAM,GAAG,GAAwB,EAAE,CAAA;IACnC,IAAI,OAAO,IAAI,CAAC,QAAQ,CAAC,GAAG,CAAC,EAAE;QAC7B,IAAI,CAAC,gDAAgD,CAAC,CAAA;QACtD,OAAO,GAAG,CAAA;KACX;IACD,KAAK,MAAM,GAAG,IAAI,GAAG,EAAE;QACrB,GAAG,CAAC,YAAY,CAAC,GAAG,CAAC,CAAC,GAAG,GAAG,CAAC,GAAG,CAAC,CAAA;KAClC;IACD,OAAO,GAAG,CAAA;AACZ,CAAC\"}"}
