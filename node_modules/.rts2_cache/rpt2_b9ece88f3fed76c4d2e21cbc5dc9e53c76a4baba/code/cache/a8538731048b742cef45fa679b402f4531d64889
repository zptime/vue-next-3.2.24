{"code":"const escapeRE = /[\"'&<>]/;\r\nexport function escapeHtml(string) {\r\n    const str = '' + string;\r\n    const match = escapeRE.exec(str);\r\n    if (!match) {\r\n        return str;\r\n    }\r\n    let html = '';\r\n    let escaped;\r\n    let index;\r\n    let lastIndex = 0;\r\n    for (index = match.index; index < str.length; index++) {\r\n        switch (str.charCodeAt(index)) {\r\n            case 34: // \"\r\n                escaped = '&quot;';\r\n                break;\r\n            case 38: // &\r\n                escaped = '&amp;';\r\n                break;\r\n            case 39: // '\r\n                escaped = '&#39;';\r\n                break;\r\n            case 60: // <\r\n                escaped = '&lt;';\r\n                break;\r\n            case 62: // >\r\n                escaped = '&gt;';\r\n                break;\r\n            default:\r\n                continue;\r\n        }\r\n        if (lastIndex !== index) {\r\n            html += str.slice(lastIndex, index);\r\n        }\r\n        lastIndex = index + 1;\r\n        html += escaped;\r\n    }\r\n    return lastIndex !== index ? html + str.slice(lastIndex, index) : html;\r\n}\r\n// https://www.w3.org/TR/html52/syntax.html#comments\r\nconst commentStripRE = /^-?>|<!--|-->|--!>|<!-$/g;\r\nexport function escapeHtmlComment(src) {\r\n    return src.replace(commentStripRE, '');\r\n}\r\n//# sourceMappingURL=escapeHtml.js.map","references":[],"map":"{\"version\":3,\"file\":\"escapeHtml.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../packages/shared/src/escapeHtml.ts\"],\"names\":[],\"mappings\":\"AAAA,MAAM,QAAQ,GAAG,SAAS,CAAA;AAE1B,MAAM,UAAU,UAAU,CAAC,MAAe;IACxC,MAAM,GAAG,GAAG,EAAE,GAAG,MAAM,CAAA;IACvB,MAAM,KAAK,GAAG,QAAQ,CAAC,IAAI,CAAC,GAAG,CAAC,CAAA;IAEhC,IAAI,CAAC,KAAK,EAAE;QACV,OAAO,GAAG,CAAA;KACX;IAED,IAAI,IAAI,GAAG,EAAE,CAAA;IACb,IAAI,OAAe,CAAA;IACnB,IAAI,KAAa,CAAA;IACjB,IAAI,SAAS,GAAG,CAAC,CAAA;IACjB,KAAK,KAAK,GAAG,KAAK,CAAC,KAAK,EAAE,KAAK,GAAG,GAAG,CAAC,MAAM,EAAE