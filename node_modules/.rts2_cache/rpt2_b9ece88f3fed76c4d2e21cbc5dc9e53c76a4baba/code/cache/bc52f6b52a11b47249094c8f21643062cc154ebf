{"code":"import { currentBlock, isBlockTreeEnabled } from '../vnode';\r\nexport function withMemo(memo, render, cache, index) {\r\n    const cached = cache[index];\r\n    if (cached && isMemoSame(cached, memo)) {\r\n        return cached;\r\n    }\r\n    const ret = render();\r\n    // shallow clone\r\n    ret.memo = memo.slice();\r\n    return (cache[index] = ret);\r\n}\r\nexport function isMemoSame(cached, memo) {\r\n    const prev = cached.memo;\r\n    if (prev.length != memo.length) {\r\n        return false;\r\n    }\r\n    for (let i = 0; i < prev.length; i++) {\r\n        if (prev[i] !== memo[i]) {\r\n            return false;\r\n        }\r\n    }\r\n    // make sure to let parent block track it when returning cached\r\n    if (isBlockTreeEnabled > 0 && currentBlock) {\r\n        currentBlock.push(cached);\r\n    }\r\n    return true;\r\n}\r\n//# sourceMappingURL=withMemo.js.map","references":["/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-core/src/vnode.ts"],"map":"{\"version\":3,\"file\":\"withMemo.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../../packages/runtime-core/src/helpers/withMemo.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,YAAY,EAAE,kBAAkB,EAAS,MAAM,UAAU,CAAA;AAElE,MAAM,UAAU,QAAQ,CACtB,IAAW,EACX,MAA6B,EAC7B,KAAY,EACZ,KAAa;IAEb,MAAM,MAAM,GAAG,KAAK,CAAC,KAAK,CAAsB,CAAA;IAChD,IAAI,MAAM,IAAI,UAAU,CAAC,MAAM,EAAE,IAAI,CAAC,EAAE;QACtC,OAAO,MAAM,CAAA;KACd;IACD,MAAM,GAAG,GAAG,MAAM,EAAE,CAAA;IAEpB,gBAAgB;IAChB,GAAG,CAAC,IAAI,GAAG,IAAI,CAAC,KAAK,EAAE,CAAA;IACvB,OAAO,CAAC,KAAK,CAAC,KAAK,CAAC,GAAG,GAAG,CAAC,CAAA;AAC7B,CAAC;AAED,MAAM,UAAU,UAAU,CAAC,MAAa,EAAE,IAAW;IACnD,MAAM,IAAI,GAAU,MAAM,CAAC,IAAK,CAAA;IAChC,IAAI,IAAI,CAAC,MAAM,IAAI,IAAI,CAAC,MAAM,EAAE;QAC9B,OAAO,KAAK,CAAA;KACb;IACD,KAAK,IAAI,CAAC,GAAG,CAAC,EAAE,CAAC,GAAG,IAAI,CAAC,MAAM,EAAE,CAAC,EAAE,EAAE;QACpC,IAAI,IAAI,CAAC,CAAC,CAAC,KAAK,IAAI,CAAC,CAAC,CAAC,EAAE;YACvB,OAAO,KAAK,CAAA;SACb;KACF;IAED,+DAA+D;IAC/D,IAAI,kBAAkB,GAAG,CAAC,IAAI,YAAY,EAAE;QAC1C,YAAY,CA