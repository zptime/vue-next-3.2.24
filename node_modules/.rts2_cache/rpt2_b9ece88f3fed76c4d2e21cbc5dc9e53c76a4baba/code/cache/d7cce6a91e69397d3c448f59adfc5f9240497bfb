{"code":"import { patchClass } from './modules/class';\r\nimport { patchStyle } from './modules/style';\r\nimport { patchAttr } from './modules/attrs';\r\nimport { patchDOMProp } from './modules/props';\r\nimport { patchEvent } from './modules/events';\r\nimport { isOn, isString, isFunction, isModelListener } from '@vue/shared';\r\nconst nativeOnRE = /^on[a-z]/;\r\nexport const patchProp = (el, key, prevValue, nextValue, isSVG = false, prevChildren, parentComponent, parentSuspense, unmountChildren) => {\r\n    if (key === 'class') {\r\n        patchClass(el, nextValue, isSVG);\r\n    }\r\n    else if (key === 'style') {\r\n        patchStyle(el, prevValue, nextValue);\r\n    }\r\n    else if (isOn(key)) {\r\n        // ignore v-model listeners\r\n        if (!isModelListener(key)) {\r\n            patchEvent(el, key, prevValue, nextValue, parentComponent);\r\n        }\r\n    }\r\n    else if (key[0] === '.'\r\n        ? ((key = key.slice(1)), true)\r\n        : key[0] === '^'\r\n            ? ((key = key.slice(1)), false)\r\n            : shouldSetAsProp(el, key, nextValue, isSVG)) {\r\n        patchDOMProp(el, key, nextValue, prevChildren, parentComponent, parentSuspense, unmountChildren);\r\n    }\r\n    else {\r\n        // special case for <input v-model type=\"checkbox\"> with\r\n        // :true-value & :false-value\r\n        // store value as dom properties since non-string values will be\r\n        // stringified.\r\n        if (key === 'true-value') {\r\n            ;\r\n            el._trueValue = nextValue;\r\n        }\r\n        else if (key === 'false-value') {\r\n            ;\r\n            el._falseValue = nextValue;\r\n        }\r\n        patchAttr(el, key, nextValue, isSVG, parentComponent);\r\n    }\r\n};\r\nfunction shouldSetAsProp(el, key, value, isSVG) {\r\n    if (isSVG) {\r\n        // most keys must be set as attribute on svg elements to work\r\n        // ...except innerHTML & textContent\r\n        if (key === 'innerHTML' || key === 'textContent') {\r\n            return true;\r\n        }\r\n        // or native onclick with function values\r\n        if (key in el && nativeOnRE.test(key) && isFunction(value)) {\r\n            return true;\r\n        }\r\n        return false;\r\n    }\r\n    // spellcheck and draggable are numerated attrs, however their\r\n    // corresponding DOM properties are actually booleans - this leads to\r\n    // setting it with a string \"false\" value leading it to be coerced to\r\n    // `true`, so we need to always treat them as attributes.\r\n    // Note that `contentEditable` doesn't have this problem: its DOM\r\n    // property is also enumerated string values.\r\n    if (key === 'spellcheck' || key === 'draggable') {\r\n        return false;\r\n    }\r\n    // #1787, #2840 form property on form elements is readonly and must be set as\r\n    // attribute.\r\n    if (key === 'form') {\r\n        return false;\r\n    }\r\n    // #1526 <input list> must be set as attribute\r\n    if (key === 'list' && el.tagName === 'INPUT') {\r\n        return false;\r\n    }\r\n    // #2766 <textarea type> must be set as attribute\r\n    if (key === 'type' && el.tagName === 'TEXTAREA') {\r\n        return false;\r\n    }\r\n    // native onclick with string value, must be set as attribute\r\n    if (nativeOnRE.test(key) && isString(value)) {\r\n        return false;\r\n    }\r\n    return key in el;\r\n}\r\n//# sourceMappingURL=patchProp.js.map","references":["/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-dom/src/modules/class.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-dom/src/modules/style.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-dom/src/modules/attrs.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-dom/src/modules/props.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-dom/src/modules/events.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/shared/src/index.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-core/src/index.ts"],"map":"{\"version\":3,\"file\":\"patchProp.js\",\"sourceRoot\":\"\",\"sources\":[\"../../../../../../packages/runtime-dom/src/patchProp.ts\"],\"names\":[],\"mappings\":\"AAAA,OAAO,EAAE,UAAU,EAAE,MAAM,iBAAiB,CAAA;AAC5C,OAAO,EAAE,UAAU,EAAE,MAAM,iBAAiB,CAAA;AAC5C,OAAO,EAAE,SAAS,EAAE,MAAM,iBAAiB,CAAA;AAC3C,OAAO,EAAE,YAAY,EAAE,MAAM,iBAAiB,CAAA;AAC9C,OAAO,EAAE,UAAU,EAAE,MAAM,kBAAkB,CAAA;AAC7C,OAAO,EAAE,IAAI,EAAE,QAAQ,EAAE,UAAU,EAAE,eAAe,EAAE,MAAM,aAAa,CAAA;AAGzE,MAAM,UAAU,GAAG,UAAU,CAAA;AAI7B,MAAM,CAAC,MAAM,SAAS,GAAoC,CACxD,EAAE,EACF,GAAG,EACH,SAAS,EACT,SAAS,EACT,KAAK,GAAG,KAAK,EACb,YAAY,EACZ,eAAe,EACf,cAAc,EACd,eAAe,EACf,EAAE;IACF,IAAI,GAAG,KAAK,OAAO,EAAE;QACnB,UAAU,CAAC,EAAE,EAAE,SAAS,EAAE,KAAK,CAAC,CAAA;KACjC;SAAM,IAAI,GAAG,KAAK,OAAO,EAAE;QAC1B,UAAU,CAAC,EAAE,EAAE,SAAS,EAAE,SAAS,CAAC,CAAA;KACrC;SAAM,IAAI,IAAI,CAAC,GAAG,CAAC,EAAE;QACpB,2BAA2B;QAC3B,IAAI,CAAC,eAAe,CAAC,GAAG,CAAC,EAAE;YACzB,UAAU,CAAC,EAAE,EAAE,GAAG,EAAE,SAAS,EAAE,SAAS,EAAE,eAAe,CAAC,CAAA;SAC3D;KACF;SAAM,IACL,GAAG,CAAC,CAAC,CAAC,KAAK,GAAG;QACZ,CAAC,CAAC,CAAC,CAAC,GAAG,GAAG,GAAG,CAAC,KAAK,CAAC,CAAC,CAAC,CAAC,EAAE,IAAI,CAAC;QAC9B,CAAC,CAAC,GAAG,CAAC,CAAC,CAAC,KAAK,GAAG;YAChB,CAAC,CAAC,CAAC,CAAC,GAAG,GAAG,GAAG,CAAC,KAAK,CAAC,CAAC,CAAC,CAAC,EAAE,KAAK,CAAC;YAC/B,CAAC,CAAC,eAAe,CAAC,EAAE,EAAE,GAAG,EAAE,SAAS,EAAE,KAAK,CAAC,EAC9C;QACA,YAAY,CACV,EAAE,EACF,GAAG,EACH,SAAS,EACT,YAAY,EACZ,eAAe,EACf,cAAc,EACd,eAAe,CAChB,CAAA;KACF;SAAM;QACL,wDAAwD;QACxD,6BAA6B;QAC7B,gEAAgE;QAChE,eAAe;QACf,IAAI,GAAG,KAAK,YAAY,EAAE;YACxB,CAAC;YAAC,EAAU,CAAC,UAAU,GAAG,SAAS,CAAA;SACpC;aAAM,IAAI,GAAG,KAAK,aAAa,EAAE;YAChC,CAAC;YAAC,EAAU,CAAC,WAAW,GAAG,SAAS,CAAA;SACrC;QACD,SAAS,CAAC,EAAE,EAAE,GAAG,EAAE,SAAS,EAAE,KAAK,EAAE,eAAe,CAAC,CAAA;KACtD;AACH,CAAC,CAAA;AAED,SAAS,eAAe,CACtB,EAAW,EACX,GAAW,EACX,KAAc,EACd,KAAc;IAEd,IAAI,KAAK,EAAE;QACT,6DAA6D;QAC7D,oCAAoC;QACpC,IAAI,GAAG,KA