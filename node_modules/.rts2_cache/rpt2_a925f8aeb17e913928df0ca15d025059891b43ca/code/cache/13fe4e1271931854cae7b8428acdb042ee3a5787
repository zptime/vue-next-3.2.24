{"code":"import { isArray } from '@vue/shared';\r\nimport { inject } from '../apiInject';\r\nimport { resolveMergedOptions } from '../componentOptions';\r\nimport { warnDeprecation } from './compatConfig';\r\nexport function createPropsDefaultThis(instance, rawProps, propKey) {\r\n    return new Proxy({}, {\r\n        get(_, key) {\r\n            __DEV__ &&\r\n                warnDeprecation(\"PROPS_DEFAULT_THIS\" /* PROPS_DEFAULT_THIS */, null, propKey);\r\n            // $options\r\n            if (key === '$options') {\r\n                return resolveMergedOptions(instance);\r\n            }\r\n            // props\r\n            if (key in rawProps) {\r\n                return rawProps[key];\r\n            }\r\n            // injections\r\n            const injections = instance.type.inject;\r\n            if (injections) {\r\n                if (isArray(injections)) {\r\n                    if (injections.includes(key)) {\r\n                        return inject(key);\r\n                    }\r\n                }\r\n                else if (key in injections) {\r\n                    return inject(key);\r\n                }\r\n            }\r\n        }\r\n    });\r\n}\r\n","references":["/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/shared/src/index.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-core/src/apiInject.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-core/src/component.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-core/src/componentOptions.ts","/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/packages/runtime-core/src/compat/compatConfig.ts"]}
