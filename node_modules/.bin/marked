#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/marked@0.7.0/node_modules/marked/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/marked@0.7.0/node_modules/marked/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/marked@0.7.0/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules:/Users/zhangpian/Documents/projects/github/node_modules:/Users/zhangpian/Documents/projects/node_modules:/Users/zhangpian/Documents/node_modules:/Users/zhangpian/node_modules:/Users/node_modules:/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/marked/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/marked/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/marked@0.7.0/node_modules/marked/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/marked@0.7.0/node_modules/marked/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/marked@0.7.0/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules:/Users/zhangpian/Documents/projects/github/node_modules:/Users/zhangpian/Documents/projects/node_modules:/Users/zhangpian/Documents/node_modules:/Users/zhangpian/node_modules:/Users/node_modules:/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/marked/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/marked/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../marked/bin/marked" "$@"
else
  exec node  "$basedir/../marked/bin/marked" "$@"
fi
