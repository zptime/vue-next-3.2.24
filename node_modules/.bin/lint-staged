#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/lint-staged@10.5.4/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules:/Users/zhangpian/Documents/projects/github/node_modules:/Users/zhangpian/Documents/projects/node_modules:/Users/zhangpian/Documents/node_modules:/Users/zhangpian/node_modules:/Users/node_modules:/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/lint-staged/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/lint-staged/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/lint-staged@10.5.4/node_modules/lint-staged/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/lint-staged@10.5.4/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/.pnpm/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules:/Users/zhangpian/Documents/projects/github/node_modules:/Users/zhangpian/Documents/projects/node_modules:/Users/zhangpian/Documents/node_modules:/Users/zhangpian/node_modules:/Users/node_modules:/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/lint-staged/bin/node_modules:/Users/zhangpian/Documents/projects/github/vue-next-3.2.24/node_modules/lint-staged/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
else
  exec node  "$basedir/../lint-staged/bin/lint-staged.js" "$@"
fi
